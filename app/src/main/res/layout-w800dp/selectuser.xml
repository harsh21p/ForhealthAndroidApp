<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="MissingDefaultResource">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="300dp"
        android:layout_height="60dp"
        android:layout_marginStart="52dp"
        android:layout_marginLeft="52dp"
        app:layout_constraintBottom_toTopOf="@+id/cardView3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.388"
        app:srcCompat="@drawable/forhealth_logo_2" />

    //MainCardLayout

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="150dp"
        android:outlineAmbientShadowColor="#5AA4F4"
        android:outlineSpotShadowColor="#559EED"
        app:cardCornerRadius="15dp"
        app:cardElevation="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"

            >

            <androidx.cardview.widget.CardView
                android:id="@+id/backbtnpa"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_centerInParent="true"
                android:foreground="@drawable/buttonlayout"
                app:cardBackgroundColor="#EEF0F6"
                app:cardCornerRadius="160dp"
                app:cardElevation="4dp"

                app:layout_constraintBottom_toBottomOf="@+id/physiotherapistcard2"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/physiotherapistcard2"
                app:layout_constraintTop_toTopOf="@+id/physiotherapistcard2">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_centerInParent="true"
                        android:layout_marginEnd="11dp"
                        android:layout_marginRight="11dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/add" />

                </RelativeLayout>


            </androidx.cardview.widget.CardView>

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/ralewaymedium"
                android:text="Select User"
                android:textColor="#082F6A"
                android:textFontWeight="800"
                android:textSize="36dp"
                app:layout_constraintBottom_toTopOf="@+id/physiotherapistcard3"
                app:layout_constraintEnd_toEndOf="@+id/physiotherapistcard3"
                app:layout_constraintStart_toStartOf="@+id/physiotherapistcard3"
                app:layout_constraintTop_toTopOf="parent" />

            <androidx.cardview.widget.CardView
                android:id="@+id/physiotherapistcard"
                android:layout_width="410dp"
                android:layout_height="580dp"
                android:layout_marginStart="188dp"
                android:layout_marginLeft="188dp"
                app:cardBackgroundColor="#FFFFFF"
                app:cardCornerRadius="19dp"
                app:cardElevation="4dp"
                app:layout_constraintBottom_toBottomOf="@+id/physiotherapistcard3"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/physiotherapistcard3">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <ImageView
                            android:id="@+id/physio"
                            android:layout_width="400dp"
                            android:layout_height="400dp"
                            android:layout_centerHorizontal="true"
                            android:layout_gravity="center"
                            android:layout_marginTop="40dp"
                            android:src="@drawable/doc" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/physio"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="40dp"
                            android:fontFamily="@font/ralewaymedium"
                            android:text="Dr. Bindu"
                            android:textColor="#082F6A"
                            android:textFontWeight="800"
                            android:textSize="30dp"


                            />

                    </RelativeLayout>

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/physiotherapistcard3"
                android:layout_width="410dp"
                android:layout_height="580dp"
                app:cardBackgroundColor="#FFFFFF"
                app:cardCornerRadius="19dp"
                app:cardElevation="4dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/physiotherapistcard2"
                app:layout_constraintStart_toEndOf="@+id/physiotherapistcard"
                app:layout_constraintTop_toTopOf="parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <ImageView
                            android:id="@+id/physio3"
                            android:layout_width="400dp"
                            android:layout_height="400dp"
                            android:layout_centerHorizontal="true"
                            android:layout_gravity="center"
                            android:layout_marginTop="40dp"
                            android:src="@drawable/doc" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/physio3"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="40dp"
                            android:fontFamily="@font/ralewaymedium"
                            android:text="Dr. Bindu"
                            android:textColor="#082F6A"
                            android:textFontWeight="800"
                            android:textSize="30dp" />

                    </RelativeLayout>

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/physiotherapistcard2"
                android:layout_width="410dp"
                android:layout_height="580dp"
                android:layout_marginEnd="188dp"
                android:layout_marginRight="188dp"
                app:cardBackgroundColor="#FFFFFF"
                app:cardCornerRadius="19dp"
                app:cardElevation="4dp"
                app:layout_constraintBottom_toBottomOf="@+id/physiotherapistcard3"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/physiotherapistcard3">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <ImageView
                            android:id="@+id/physio2"
                            android:layout_width="400dp"
                            android:layout_height="400dp"
                            android:layout_centerHorizontal="true"
                            android:layout_gravity="center"
                            android:layout_marginTop="40dp"
                            android:src="@drawable/doc" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/physio2"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="40dp"
                            android:fontFamily="@font/ralewaymedium"
                            android:text="Dr. Bindu"
                            android:textColor="#082F6A"
                            android:textFontWeight="800"
                            android:textSize="30dp"


                            />

                    </RelativeLayout>

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.cardview.widget.CardView>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/physiotherapistcard">

                <View
                    android:id="@+id/ellipse_104"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:background="@drawable/circle4"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <View
                    android:id="@+id/ellipse_105"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginStart="20dp"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/circle4"
                    app:layout_constraintBottom_toBottomOf="@+id/ellipse_104"
                    app:layout_constraintStart_toEndOf="@+id/ellipse_104"
                    app:layout_constraintTop_toTopOf="@+id/ellipse_104"
                    app:layout_constraintVertical_bias="0.0" />

                <View
                    android:id="@+id/ellipse_106"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginStart="20dp"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/circle4"
                    app:layout_constraintBottom_toBottomOf="@+id/ellipse_104"
                    app:layout_constraintStart_toEndOf="@+id/ellipse_105"
                    app:layout_constraintTop_toTopOf="@+id/ellipse_104" />

                <View
                    android:id="@+id/ellipse_107"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginStart="20dp"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/circle"
                    app:layout_constraintBottom_toBottomOf="@+id/ellipse_104"
                    app:layout_constraintStart_toEndOf="@+id/ellipse_106"
                    app:layout_constraintTop_toTopOf="@+id/ellipse_104" />


            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>



</androidx.constraintlayout.widget.ConstraintLayout>